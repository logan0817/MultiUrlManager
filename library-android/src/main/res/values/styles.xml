        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/base_url_color_primary</item>
        <item name="colorPrimaryDark">@color/base_url_color_primaryDark</item>
        <item name="colorAccent">@color/base_url_color_accent</item>
        <item name="base_url_bg_color">@color/base_url_bg_color</item>
